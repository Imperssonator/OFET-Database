function [hScat,hMarks,ax] = StdFig()

StdMob = [];
StdGroups = {};
TotDev = 0;
Constants = GenerateEmptyConstants();

[MobSort, GroupSort, Filtered] = OFETSearch({},'Year');
StdMob = [StdMob; MobSort];
for i = TotDev+1:length(StdMob)
    StdGroups{i,1} = 'All Devices';
end
TotDev = length(StdMob);

Constants.Solv1={'CHCl3'};
Constants.VFSolv1 = NaN;
[MobSort, GroupSort, Filtered] = OFETSearch({'Solv1','CHCl3'; 'VFSolv1',NaN},'Year');
StdMob = [StdMob; MobSort];
for i = TotDev+1:length(StdMob)
    StdGroups{i,1} = 'CHCl3';
end
TotDev = length(StdMob);

Constants.Depo={'Spun'};
[MobSort, GroupSort, Filtered] = OFETSearch({'Solv1','CHCl3'; 'VFSolv1',NaN; 'Depo', 'SPUN'},'Year');
StdMob = [StdMob; MobSort];
for i = TotDev+1:length(StdMob)
    StdGroups{i,1} = 'Spin-coated';
end
TotDev = length(StdMob);

Constants.
[MobSort, GroupSort, Filtered] = OFETSearch({'Solv1','CHCl3'; 'VFSolv1',NaN; 'Depo','SPUN'; 'OFETConfig','BGBC'; 'ElectrodeMat','Au'},'Year');
StdMob = [StdMob; MobSort];
for i = TotDev+1:length(StdMob)
    StdGroups{i,1} = 'BGBC';
end
TotDev = length(StdMob);

[MobSort, GroupSort, Filtered] = OFETSearch({'Solv1','CHCl3'; 'VFSolv1',NaN; 'Depo','SPUN'; 'OFETConfig','BGBC'; 'ElectrodeMat','Au'; 'Mn',[20 Inf]},'Year');
StdMob = [StdMob; MobSort];
for i = TotDev+1:length(StdMob)
    StdGroups{i,1} = 'Mn > 20kD';
end
TotDev = length(StdMob);

[MobSort, GroupSort, Filtered] = OFETSearch({'Solv1','CHCl3'; 'VFSolv1',NaN; 'Depo','SPUN'; 'OFETConfig','BGBC'; 'ElectrodeMat','Au'; 'Mn',[20 Inf]; 'AnnTime',NaN},'Year');
StdMob = [StdMob; MobSort];
for i = TotDev+1:length(StdMob)
    StdGroups{i,1} = 'No Annealing';
end
TotDev = length(StdMob);

[MobSort, GroupSort, Filtered] = OFETSearch({'Solv1','CHCl3'; 'VFSolv1',NaN; 'Depo','SPUN'; 'OFETConfig','BGBC'; 'ElectrodeMat','Au'; 'Mn',[20 Inf]; 'AnnTime',NaN; 'SonicationTime',NaN; 'AgeTime',NaN},'Year');
StdMob = [StdMob; MobSort];
for i = TotDev+1:length(StdMob)
    StdGroups{i,1} = 'No Pre-treatment';
end
TotDev = length(StdMob);

[MobSort, GroupSort, Filtered] = OFETSearch({'Solv1','CHCl3'; 'VFSolv1',NaN; 'Depo','SPUN'; 'OFETConfig','BGBC'; 'ElectrodeMat','Au'; 'Mn',[20 Inf]; 'AnnTime',NaN; 'SonicationTime',NaN; 'AgeTime',NaN; 'ChanLen',[10 Inf]},'Year');
StdMob = [StdMob; MobSort];
for i = TotDev+1:length(StdMob)
    StdGroups{i,1} = 'Channel > 10µm';
end
TotDev = length(StdMob);

[hScat,hMarks,ax] = GoodBox(StdGroups,StdMob);

ax.XTickLabels = {'All Devices'; 'Chloroform'; 'Spin-coated'; 'BGBC'; 'Mn > 20kD'; 'No Annealing'; 'No Pre-treatment'; 'Channel < 10µm'};
ax.PlotBoxAspectRatioMode = 'auto';

end